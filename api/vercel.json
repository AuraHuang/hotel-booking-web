{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://booking-web-backend-398712.de.r.appspot.com"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}